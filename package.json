{
    "name": "@zzzz-/select",
    "description": "选择组件",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/zkp442910864/select"
    },
    "homepage": "https://github.com/zkp442910864/select",
    "keywords": [
        "ScrollBox",
        "Select",
        "TreeSelect"
    ],
    "scripts": {
        "start": "webpack serve --env CUSTOM_NODE_ENV=development --mode=development --config config/index.js",
        "start:no-mock": "webpack serve --env CUSTOM_NODE_ENV=development mock=no --mode=development --config config/index.js",
        "build:web": "webpack build --env CUSTOM_NODE_ENV=production --mode=production --config config/index.js",
        "build": "npm run build:lib",
        "clear": "gulp run",
        "build:umd": "webpack build --env CUSTOM_NODE_ENV=production lib=umd --mode=production --config config/index.js",
        "build:es": "babel src --out-dir dist/es --extensions .tsx --extensions .ts --ignore src/**/*.stories.tsx --ignore src/stories/**/* --ignore src/App.tsx --ignore src/main.ts --source-maps --copy-files --no-copy-ignored",
        "build:tsc-types": "tsc -p tsconfig.build.json",
        "build:lib": "npm run clear && npm run build:tsc-types && npm run build:es && npm run build:umd",
        "publish:patch": "npm run build:lib && npm version patch && npm publish",
        "copy:less": "copyfiles -u 1 src/**/*.scoped.less lib/",
        "copy:types": "copyfiles -u 1 src/types/**/* types/",
        "jest": "jest --passWithNoTests",
        "storybook": "start-storybook -p 6006 -s public",
        "build:docs": "build-storybook -o ./docs -s public"
    },
    "main": "dist/umd/index.js",
    "module": "dist/es/tsc.js",
    "types": "dist/types/tsc.d.ts",
    "peerDependencies": {
        "react": ">=16.0.0 || >=17.0.0 || >=18.0.0",
        "react-dom": ">=16.0.0 || >=17.0.0 || >=18.0.0"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.19.3",
        "@babel/plugin-transform-regenerator": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.19.1",
        "@babel/preset-env": "^7.19.3",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/runtime": "^7.19.0",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
